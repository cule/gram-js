{
  "name": "@gram-data/gram-parse",
  "version": "0.2.1",
  "license": "MIT",
  "main": "dist/index.js",
  "unpkg": "dist/gram-parse.umd.production.min.js",
  "module": "dist/gram-parse.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "run-s build:near lint:fix build:dist",
    "build:near": "nearleyc src/gram.ne -o src/gram-grammar.ts",
    "build:dist": "tsdx build --format umd,esm,cjs",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "railroad": "nearley-railroad ./src/gram.ne -o railroad.html",
    "build:demo": "shx cp dist/gram-parse.umd.development.* ./public",
    "demo": "serve ./public/",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Andreas Kollegger",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/moo": "^0.5.3",
    "@types/nearley": "^2.11.1",
    "@types/unist": "^2.0.3",
    "@typescript-eslint/parser": "^3.6.1",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "serve": "^11.3.2",
    "shx": "^0.3.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7",
    "unist-util-inspect": "^6.0.0"
  },
  "dependencies": {
    "@gram-data/gram-ast": "0.2.1",
    "@gram-data/gram-builder": "0.2.1",
    "moo": "^0.5.1",
    "nearley": "^2.19.5",
    "unified": "^9.0.0",
    "vfile": "^4.1.1"
  },
  "gitHead": "a255da2f7cfa6bd16f637d1241d17e10510e60c0",
  "publishConfig": {
    "access": "public"
  }
}
