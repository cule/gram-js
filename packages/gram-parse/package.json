{
  "name": "@gram-data/gram-parse",
  "version": "0.1.6",
  "license": "MIT",
  "main": "dist/index.js",
  "unpkg": "dist/gram-parse.umd.production.min.js",
  "module": "dist/gram-parse.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "run-s build:near lint:fix build:dist",
    "build:near": "nearleyc src/gram.ne -o src/gram-grammar.ts",
    "build:dist": "tsdx build --format umd,esm,cjs",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "run-s build",
    "railroad": "nearley-railroad ./src/gram.ne -o docs/railroad.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Andreas Kollegger",
  "devDependencies": {
    "@types/moo": "^0.5.3",
    "@types/nearley": "^2.11.1",
    "@types/unist": "^2.0.3",
    "@typescript-eslint/parser": "^3.5.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@gram-data/gram-ast": "^0.1.6",
    "@gram-data/gram-builder": "^0.1.6",
    "moo": "^0.5.1",
    "nearley": "^2.19.4",
    "unified": "^9.0.0",
    "vfile": "^4.1.1"
  },
  "gitHead": "beda5178ad8e13dbeb43eef83a96bcaa0173ac5f",
  "publishConfig": {
    "access": "public"
  }
}
