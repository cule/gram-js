{
  "name": "@gram-data/gram-graph",
  "author": "Andreas Kollegger",
  "keywords": [
    "graph",
    "neo4j",
    "gram"
  ],
  "version": "0.2.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "run-s build:lib build:demo",
    "build:lib": "tsdx build --format umd,esm,cjs --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "build:demo": "shx cp dist/gram-graph.umd.development.* ./public",
    "demo": "serve ./public/",
    "prepare": "npm run build",
    "update": "ncu -u",
    "update:check": "ncu"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/gram-graph.esm.js",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "husky": "^4.3.0",
    "npm-check-updates": "^9.2.3",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "serve": "^11.3.2",
    "shx": "^0.3.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "gitHead": "0f6d560e002650f6c293c0737ddc2539d78fbbff",
  "publishConfig": {
    "access": "public"
  }
}
