  "module": "dist/gram-graph.esm.js",
  "devDependencies": {
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "serve": "^11.3.2",
    "shx": "^0.3.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  },
