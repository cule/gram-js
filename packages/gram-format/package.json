{
  "name": "@gram-data/gram-format",
  "homepage": "https://github.com/gram-data/gram-js/tree/master/packages/gram-format",
  "bugs": "https://github.com/gram-data/gram-js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/gram-data/gram-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.12",
  "license": "Apache-2.0",
  "author": "Andreas Kollegger",
  "main": "dist/index.js",
  "module": "dist/gram-format.esm.js",
  "unpkg": "dist/gram-format.umd.production.min.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=0.12"
  },
  "bin": {
    "gram-lint": "./bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist features/support/js .rts2_cache*",
    "build": "run-s clean near lint-fix build-dist",
    "build-dist": "tsdx build --format umd,cjs,esm",
    "build-cucumber": "tsc --project tsconfig-cucumber.json",
    "lint": "tsdx lint src",
    "lint-fix": "tsdx lint src --fix",
    "docs": "run-s typedoc railroad",
    "typedoc": "typedoc --out docs src",
    "railroad": "nearley-railroad ./src/gram.ne -o docs/railroad.html",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "near": "nearleyc src/gram.ne -o src/gram-parse.ts",
    "test:features": "run-s build-cucumber test:cucumber",
    "test:cucumber": "cucumber-js features",
    "release": "np --no-tests --no-release-draft --no-yarn"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^25.2.3",
    "@types/mathjs": "^6.0.5",
    "@types/nearley": "^2.11.1",
    "@types/node": "^14.0.11",
    "@types/shortid": "^0.0.29",
    "@types/unist": "^2.0.3",
    "@typescript-eslint/parser": "^3.1.0",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "husky": "^4.2.5",
    "np": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5",
    "unist-util-find": "^1.0.1",
    "unist-util-size": "^2.0.0"
  },
  "dependencies": {
    "base-x": "^3.0.8",
    "commander": "^5.1.0",
    "fp-ts": "^2.6.2",
    "nearley": "^2.19.3",
    "shortid": "^2.2.15",
    "unified": "^9.0.0",
    "unist-builder": "^2.0.3",
    "unist-util-inspect": "^6.0.0",
    "unist-util-visit": "^2.0.2",
    "unist-util-visit-children": "^1.1.4"
  }
}
