{
  "name": "@gram-data/gram-lint",
  "keywords": [
    "graph",
    "neo4j",
    "gram"
  ],
  "version": "0.2.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gram-data/gram-js.git"
  },
  "homepage": "https://github.com/gram-data/gram-js/tree/main/packages/gram-lint#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "gram-lint": "./bin/cli.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "npm run build",
    "update": "ncu -u",
    "update:check": "ncu"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Andreas Kollegger",
  "module": "dist/gram-lint.esm.js",
  "devDependencies": {
    "@gram-data/gram-ast": "^0.2.8",
    "@gram-data/gram-identity": "^0.2.8",
    "@types/jest": "^26.0.15",
    "husky": "^4.3.0",
    "npm-check-updates": "^10.2.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "serve": "^11.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@gram-data/gram-parse": "^0.2.8",
    "@gram-data/gram-stringify": "^0.2.8",
    "unified": "^9.2.0",
    "unified-args": "^8.1.0"
  },
  "gitHead": "0f6d560e002650f6c293c0737ddc2539d78fbbff",
  "publishConfig": {
    "access": "public"
  }
}
