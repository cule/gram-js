{
  "name": "gram.js API",
  "mode": "modules",
  "readme": "README.md",
  "inputFiles": [
    "packages/gram-ast/src",
    "packages/gram-builder/src",
    "packages/gram-identity/src",
    "packages/gram-ops/src",
    "packages/gram-parse/src",
    "packages/gram-stringify/src",
    "packages/gram-value/src",
    "packages/gram/src"
  ],
  "out": "docs/api",
  "ignoreCompilerErrors": false,
  "listInvalidSymbolLinks": true,
  "preserveConstEnums": true,
  "exclude": "*.(spec|test).ts",
  "excludePrivate": true,
  "excludeNotExported": true,
  "external-modulemap": "packages\/([^\/]+)\/",
  "hideGenerator": true,
  "stripInternal": false
}
